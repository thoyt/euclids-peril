pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
function _init()
 tiles = {}
 par = {}
 par.n = 8
 par.m = 8
 par.w = 2
 par.t = -1
 hero = {}
 hero.x = 1
 hero.y = 1
 w = 16
 compute_tiles()
end

function move_hero()
 if btnp(0) then
  hero.x -= 1
 end
 if btnp(1) then
  hero.x += 1
 end
 if btnp(2) then
  hero.y -= 1
 end
 if btnp(3) then
  hero.y += 1
 end
end

function compute_tiles()
 for i=0,par.n do
  tiles[i] = {}
  for j=0,par.m do
   tiles[i][j] = {}
   tiles[i][j] = flr(rnd(3))
  end
 end
end

function draw_tiles()
 for i=0,par.n do
  for j=0,par.m do
   spr(tiles[i][j]*2,i*16,j*16,2,2)
  end
 end
end

function _update()
 par.t = par.t+1
end

function _draw()
 move_hero()
 draw_tiles()
 spr(6,hero.x*w,hero.y*w,2,2)
end


