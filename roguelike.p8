pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
function _init()
 tiles = {}
 par = {} -- params we use only sometimes
 par.n = 8 -- tile grid dims
 par.m = 8
 hero = {}
 hero.x = 0
 hero.y = 0
 hero.spr = 6 -- sprite location
 hero.canmove = true
 t = 0
 w = 16 -- the width of our tiles
 compute_tiles()
 tile_types = {}
 tile_types.magic = 2
end

function move_hero()
 if btnp(0) and hero.x > 0 then
  hero.x -= 1
 end
 if btnp(1) and hero.x < par.n then
  hero.x += 1
 end
 if btnp(2) and hero.y > 0 then
  hero.y -= 1
 end
 if btnp(3) and hero.y < par.m then
  hero.y += 1
 end
 check_tile()
end

function compute_tiles()
 for i=0,par.n do
  tiles[i] = {}
  for j=0,par.m do
   tiles[i][j] = {}
   tiles[i][j] = flr(rnd(3))
  end
 end
end

function draw_tiles()
 for i=0,par.n do
  for j=0,par.m do
   spr(tiles[i][j]*2,
    i*w,j*w,2,2)
  end
 end
end

function check_tile()
 if tiles[hero.x][hero.y]==tile_types.magic then
  sfx(0)
 end
end

function _update()
 t+=1
end

function _draw()
 cls()
 draw_tiles()
 move_hero()
 spr(hero.spr,
  hero.x*w,hero.y*w,2,2)
end

